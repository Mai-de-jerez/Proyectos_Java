package POO;

import java.util.Date;

public class EjemploAutomovilEstatico {

	public static void main(String[] args) {
		
		// Vemos detalle
		// nuevo objeto de la clase Automovil
				Automovil subaru = new Automovil("Subaru", "Impreza");
				subaru.setColor("rosa"); 
				subaru.setCilindrada(2.0); 
				
				// objeto numero 2
				Automovil mazda = new Automovil("Mazda","BT-50", "rojo", 3.0); 
				
				// objetos numero 3 y 4
				Automovil nissan = new Automovil("Nissan", "Navara", "Gris Metalizado", 3.5, 50);
				Automovil opel = new Automovil("Opel", "Vectra");
				
				// Vemos el resultado
				System.out.println("\n--- Caracter√≠sticas ---");
			
				System.out.println(subaru.verDetalle());
				System.out.println(mazda.verDetalle());
				System.out.println(nissan.verDetalle());
				System.out.println(opel.verDetalle());
			
				
				

	}

}
