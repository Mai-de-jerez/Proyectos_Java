public class ControlFlota {

	public static void main(String[] args) {
		
	
        Nave nave1 = new Nave("Odisea", Faccion.CINTURON,5, true);
        
        
        System.out.println("--- PRUEBA DE VISUALIZACIÃ“N DE NAVE ---\n");
  
