package POO;

public enum Color {
	ROJO("Rojo"),
	AMARILLO("Amarillo"),
	AZUL("Azul"),
	BLANCO("Blanco"),
	GRIS("Gris Oscuro"),
	NARANJA("Naranja");
	
	private final String color;
	
	Color(String color){
		this.color=color;
	}

	public String getColor() {
		return color;
	}

	@Override
	public String toString() {
		// TODO Esbozo de método generado automáticamente
		return "Color{"+ "color=' "+color+'\''+'}';
	}
	
	
}
