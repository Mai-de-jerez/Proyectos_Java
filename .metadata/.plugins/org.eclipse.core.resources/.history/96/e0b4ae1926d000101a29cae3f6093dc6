package HolaMundo;

public class HolaMundo {

	public static void main(String[] args) {
		// Variables:
		// Tipo String
		String saludar = "Hola Mundo desde Java";
		System.out.print(saludar);
		
		// Tipo n√∫mericos
		// Enteros
		int numero = 10;
		System.out.print(numero);
		// Booleanos (true o false)
		boolean valor = true;
		
		int numero2=5;
		if (valor) {
			System.out.print(numero);
			numero2 = 10;
		}
		
		System.out.print(numero2); 

	} 

}
