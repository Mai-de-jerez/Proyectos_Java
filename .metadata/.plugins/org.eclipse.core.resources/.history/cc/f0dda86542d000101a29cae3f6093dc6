package POO;

public class EjemploAutomovil {

	public static void main(String[] args) {
		
		
		// nuevo objeto de la clase Automovil
		Automovil auto = new Automovil();
		
		// asignar atributos
		auto.fabricante = "Subaru";
		auto.color = "rosa"; // podemos cambiar a rosa aunque pusieramos rojo en el constructor
		auto.modelo = "Impresa";
		auto.cilindrada = 2.0;
		
		Automovil auto2 = new Automovil();
		// asignar atributos
		auto2.fabricante = "Mazda";
		auto2.color = "azul"; // podemos cambiar a rosa aunque pusieramos rojo en el constructor
		auto2.modelo = "BT-50";
		auto2.cilindrada = 3.0;
		
		
		// vemos el resultado
		System.out.println("Fabricante auto: "+ auto.fabricante);
		System.out.println("Modelo auto: "+ auto.modelo);
		System.out.println("Color auto: "+ auto.color);
		System.out.println("Cilindrada auto: "+ auto.cilindrada);
		
		System.out.println("\nFabricante auto2: "+ auto2.fabricante);
		System.out.println("Modelo auto2: "+ auto2.modelo);
		System.out.println("Color auto2: "+ auto2.color);
		System.out.println("Cilindrada auto2: "+ auto2.cilindrada);
	}

}
