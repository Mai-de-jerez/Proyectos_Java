package POO;

public class EjemploAutomovil {

	public static void main(String[] args) {
		
		// nuevo objeto de la clase Automovil
		Automovil auto = new Automovil();
		System.out.println("Fabricante auto: "+ auto.fabricante);
		System.out.println("Modelo auto: "+ auto.modelo);
		System.out.println("Color auto: "+ auto.color);
		System.out.println("Cilindrada auto: "+ auto.cilindrada);
	}

}
